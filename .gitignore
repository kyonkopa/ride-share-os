# Rails backend .gitignore

# OS
.DS_Store

/node_modules

# Bundler
/.bundle
/vendor/bundle
/vendor/cache

# Logs and temp files
/log/*
!/log/.keep
/tmp/*
!/tmp/.keep

# Storage (Active Storage, etc.)
/storage/*
!/storage/.keep

# Assets (compiled)
/public/assets
/public/packs
/public/packs-test
/public/packs-manifest.json

# Credentials / keys
/config/master.key

# Databases (if using sqlite locally)
/db/*.sqlite3
/db/*.sqlite3-journal
/db/*.sqlite3-* 

# Test / coverage artifacts
/coverage
/spec/examples.txt
.rspec_status
capybara-*.html

# Debugger / history
.byebug_history

# Editors / IDE
.idea/
.vscode/

# Env files
.env
.env.*
.env.local
.env.*.local

# Swap files
*.swp
*.swo

